{
  "modeName": "thulbModus",
  "subModes": [
    {
      "id": 0,
      "format": "thulb_gross",
      "useRegEx": true,
      "regEx": "(.*):(.*):(.*):(.*):(.*):(.*)",
      "delimiter": "",
      "result": ["$1", "$2", "$3", "$4", "$5", "$6"]
    },
    {
      "id": 1,
      "format": "thulb_klein_1",
      "useRegEx": true,
      "regEx": "^(.*)$",
      "delimiter": "",
      "result": ["$1"]
    },
    {
      "id": 2,
      "format": "thulb_klein",
      "useRegEx": true,
      "regEx": "^(\\w+\\s[\\w\\.\\,]+)([\\s\\/]?)([^\\/\\:]*)([\\/\\:]?)(.*)$",
      "delimiter": "",
      "result": ["$1$2", "$3$4", "$5"]
    }
  ]
}